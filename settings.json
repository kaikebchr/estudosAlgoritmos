{
    "window.zoomLevel": 1,
    "workbench.colorTheme": "GitHub Light Default",
    // "workbench.colorTheme": "Tokyo Night Dark",
    "workbench.iconTheme": "symbols",
    "workbench.productIconTheme": "material-product-icons",
    "editor.wordWrap": "on",
    "editor.fontLigatures": true,
    // "editor.formatOnSave": true,
    "editor.fontSize": 14,
    "terminal.integrated.fontSize": 14,
    "editor.lineHeight": 1.5,
    "editor.fontFamily": "Space Mono",
    "scm.inputFontFamily": "Space Mono",
    "chat.editor.fontFamily": "Space Mono",
    "debug.console.fontFamily": "Space Mono",
    "editor.codeLensFontFamily": "Space Mono",
    "notebook.output.fontFamily": "Space Mono",
    "markdown.preview.fontFamily": "Space Mono",
    "editor.inlayHints.fontFamily": "Space Mono",
    "terminal.integrated.fontFamily": "Space Mono",
    "editor.inlineSuggest.fontFamily": "Space Mono",
    "editor.experimentalInlineEdit.fontFamily": "Space Mono",
    "workbench.startupEditor": "newUntitledFile",
    "editor.renderLineHighlight": "gutter",
    "workbench.editor.labelFormat": "short",
    "explorer.compactFolders": false,
    "editor.semanticHighlighting.enabled": false,
    "breadcrumbs.enabled": false,
    "editor.minimap.enabled": false,
    "symbols.hidesExplorerArrows": false,
    "window.commandCenter": false,
    "workbench.layoutControl.enabled": false
}