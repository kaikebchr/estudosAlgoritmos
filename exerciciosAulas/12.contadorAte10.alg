algoritmo "contadorAte10"
var
    contador: inteiro

inicio
    escreval("Iniciando contagem até o número 10...")
    escreval("")

    contador <- 0
    enquanto (contador <= 10) faca
        escreval(contador, "...")
        contador <- contador + 1
    fimenquanto

    escreval("")
    escreval("Contagem finalizada.")

fimalgoritmo
